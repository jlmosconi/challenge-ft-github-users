{
  "name": "github-users",
  "version": "0.0.1",
  "build": 1,
  "private": true,
  "scripts": {
    "start": "yarn react-native start --reset-cache",
    "android": "yarn react-native run-android --mode=developmentDebug --appIdSuffix 'dev'",
    "android:release": "yarn react-native run-android --mode=developmentRelease --appIdSuffix 'dev'",
    "android:prod": "yarn react-native run-android --mode=productionDebug",
    "android:prod-release": "yarn react-native run-android --mode=productionRelease",
    "ios": "yarn react-native run-ios --scheme \"GithubUsers Dev\" --mode \"Debug Dev\"",
    "ios:release": "yarn react-native run-ios --scheme \"GithubUsers Dev\" --mode \"Release Dev\" ",
    "ios:prod": "yarn react-native run-ios --scheme \"GithubUsers\" --mode \"Debug\"",
    "ios:prod-release": "yarn react-native run-ios --scheme \"GithubUsers\" --mode \"Release\"",
    "build:ios": "yarn react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "build:android": "yarn react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "lint": "eslint .",
    "test": "jest",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "5.1.6",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-firebase/analytics": "22.4.0",
    "@react-native-firebase/app": "22.4.0",
    "@react-native-firebase/crashlytics": "22.4.0",
    "@react-native-firebase/perf": "22.4.0",
    "@react-navigation/native": "7.1.14",
    "@react-navigation/stack": "7.4.2",
    "@reduxjs/toolkit": "2.8.2",
    "apisauce": "3.2.0",
    "i18n-js": "4.5.1",
    "react": "19.1.0",
    "react-content-loader": "7.0.2",
    "react-native": "0.80.1",
    "react-native-config": "1.5.5",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "2.27.1",
    "react-native-localize": "3.5.1",
    "react-native-reanimated": "3.18.0",
    "react-native-safe-area-context": "5.5.2",
    "react-native-screens": "4.11.1",
    "react-native-svg": "15.12.0",
    "react-redux": "9.2.0",
    "redux-persist": "6.0.0",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native-community/datetimepicker": "^8.4.2",
    "@react-native-community/slider": "^4.5.7",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@storybook/addon-ondevice-actions": "^9.0.18",
    "@storybook/addon-ondevice-controls": "^9.0.18",
    "@storybook/react-native": "^9.0.18",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-loader": "^8.4.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-dom": "19.1.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "19.1.0",
    "storybook": "^9.0.18",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
