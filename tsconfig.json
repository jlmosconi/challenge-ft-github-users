{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "types": ["node", "jest"],
    "strictNullChecks": true,
    "baseUrl": "./app",
    "paths": {
      "@assets/*": ["../assets/*"],
      "@adapters/*": ["adapters/*"],
      "@components/*": ["components/*"],
      "@config/*": ["config/*"],
      "@helpers/*": ["helpers/*"],
      "@hooks/*": ["hooks/*"],
      "@navigators/*": ["navigators/*"],
      "@screens/*": ["screens/*"],
      "@services/*": ["services/*"],
      "@store/*": ["store/*"],
      "types/*": ["types/*"],
      "@utils/*": ["utils/*"]
    }
  },
  "include": ["index.js", "app", "test", ".rnstorybook", "__tests__", "__mocks__", "jest.config.js"]
}
